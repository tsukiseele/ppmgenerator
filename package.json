{
  "name": "ppmgenerator",
  "version": "1.0.0",
  "main": "./src/main.ts",
  "type": "module",
  "scripts": {
    "start": "bun ./src/main.ts",
    "run-exe": "./PPMG.exe",
    "build-windows-x64": "bun build --compile --target=bun-windows-x64 ./src/main.ts --outfile PPMGenerator-x86_64",
    "build-linux-x64": "bun build --compile --target=bun-linux-x64 ./src/main.ts --outfile PPMG",
    "build-linux-arm64": "bun build --compile --target=bun-linux-arm64 ./src/main.ts --outfile PPMG",
    "build-darwin-x64": "bun build --compile --target=bun-darwin-arm64 ./src/main.ts --outfile PPMG",
    "build-darwin-arm64": "bun build --compile --target=bun-darwin-arm64 ./src/main.ts --outfile PPMG"
  },
  "outputPath": "dist",
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "archiver": "^7.0.1",
    "image-size": "^1.1.1"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3"
  }
}
